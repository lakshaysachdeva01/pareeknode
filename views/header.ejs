                    <i class="fa fa-angle-down"></i>
                  </button>
                  <ul class="dropdown_menu">
                    <div class="category-container">
                      <% categorylist.forEach(item => { %>
                        <li class="a">
                            <a class="navlink" href="/category-items?category=<%= item._id %>" data-category-id="<%= item._id %>"><%= item.name %></a>
                        </li>
                    <% }) %>
                    </div>
                  </ul>
