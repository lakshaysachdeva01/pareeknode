              <li class="baritems <%= pathPart === 'colour-palette' ? 'active' : '' %>"><a class="navlink" href="/colour-palette">COLOUR PALETTE</a></li>
              <li class="baritems <%= pathPart === 'contact' ? 'active' : '' %>"><a class="navlink" href="/contact">CONTACT US</a></li>
          </ul>
      </div>
      
    
          <button type="button" onclick="openmenu()" id="openn"><i class="flaticon-sort"></i></button>
          <div id="showmenufull"> </div> 
          <div id="showmenu" class="navbar-mobile-wrapper">
            <div class="logo_menu">
              <a href="/"><img src="/assets/imgs/logo/footer-logo.png" alt="logo"></a> 
           </div>
          
              <ul class="menubar-mobile">
                <li><a class="navlink" href="/">Home</a></li>
                <li><a class="navlink" href="/about">About Us</a></li>
                <li>
                  <a class="navlink" href="/products">Products</a>
