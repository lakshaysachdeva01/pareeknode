                  <ul class="dropdown">
                      <div class="category-container">
                        <% categorylist.forEach(item => { %>
                          <li class="a">
                              <a class="navlink" href="/category-items?category=<%= item._id %>"><%= item.name %></a>
                          </li>
                      <% }) %>
                      </div>
                  </ul>
              </li>
              <li class="baritems <%= pathPart === 'services' ? 'active' : '' %>"><a class="navlink" href="/services">SERVICES</a></li>
              <li class="baritems <%= pathPart === 'dealership' ? 'active' : '' %>"><a class="navlink" href="/dealership">DEALERSHIP</a></li>
